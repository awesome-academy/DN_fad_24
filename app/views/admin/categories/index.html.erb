<% provide :title, t(".title") %>
<div class="content">
  <%= link_to t(".add_category"), new_admin_category_path, class: "add_category" %>
  <table class="table">
    <thead class=" text-primary">
      <th>
        <%= t(".name") %>
      </th>
      <th>
        <%= t(".function") %>
      </th>
    </thead>
    <tbody>
      <%= render @categories %>
    </tbody>
  </table>
  <%= will_paginate %>
</div>
